@model IEnumerable<GeoPedia.Models.City>

<div>
@foreach(var item in Model)
{
    <h3>@Html.ActionLink(item.Name, "Details", "City", new { id = item.Id}, null) <small>(@Html.ActionLink("Edit", "Edit", "City", new { id = item.Id }, null) | @Html.ActionLink("Delete", "Delete", "City", new { id = item.Id}, null))</small></h3>
    <p>@Html.DisplayNameFor(m => m.Population): @item.Population</p>
    <p>@Html.DisplayNameFor(m => m.Mayor): @item.Mayor</p>
}
</div>
